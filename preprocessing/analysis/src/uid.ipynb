   "source": [
    "import pandas as pd\n",
    "import uuid\n",
    "\n",
    "df = pd.read_csv(\"../data/input/misconduct.csv\")\n",
    "\n",
    "# Generate unique IDs\n",
    "df['id'] = [uuid.uuid4() for _ in range(len(df))]\n",
    "\n",
    "df = df.fillna(\"\")\n",
    "df\n",
    "\n",
    "# Print the DataFrame with the new 'id' column\n",
    "df.to_csv(\"../data/output/misconduct_uid.csv\", index=False)"
   ]
